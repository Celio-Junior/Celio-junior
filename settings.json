{
  "editor.fontFamily": "Cascadia Code",
  "workbench.colorTheme": "Illusion",
  "workbench.iconTheme": "material-icon-theme",
  "terminal.integrated.fontFamily": "Hack Nerd Font",
  "editor.wordWrap": "on",
  "editor.renderLineHighlight": "gutter",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.rulers": [
    95,
    110
  ],
  "terminal.integrated.fontSize": 14,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "package.json": "package-lock.json, tsconfig*, webpack*, .gitignore, vite*",
    ".eslintrc*": ".prettier*, .editorconfig"
  },
  "code-runner.runInTerminal": true,
  "code-runner.saveAllFilesBeforeRun": true,
  "code-runner.saveFileBeforeRun": true,
  "code-runner.clearPreviousOutput": true,
  "editor.guides.bracketPairs": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "security.workspace.trust.untrustedFiles": "open",
  "debug.terminal.clearBeforeReusing": true,
  "workbench.startupEditor": "none",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll": "explicit"
  },
  "editor.tabSize": 2,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "css.styleSheets": [
    "https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css",
    "./*.css"
  ],
  "database-client.autoSync": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.inlineSuggest.suppressSuggestions": true,
  "git.openRepositoryInParentFolders": "never",
  "editor.linkedEditing": true,
  "liveServer.settings.AdvanceCustomBrowserCmdLine": "/mnt/c/Users/Celio/AppData/Local/Programs/Opera GX/opera.exe",
}
